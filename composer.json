{
    "name": "phpactor/reference-finder",
    "description": "Base library for finding definitions",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "/home/daniel/www/phpactor/name"
        }
    ],
    "require": {
        "phpactor/text-document": "~1.0",
        "webmozart/path-util": "^2.3",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpstan/phpstan": "~0.11.0",
        "phpunit/phpunit": "~7.0",
        "friendsofphp/php-cs-fixer": "~2.15.0"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\ReferenceFinder\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\ReferenceFinder\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "integrate": [
            "./vendor/bin/php-cs-fixer fix",
            "./vendor/bin/phpstan analyse lib -c phpstan.neon",
            "./vendor/bin/phpunit"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
